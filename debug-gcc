- without CMSIS

arm-none-eabi-gcc -Iinclude/ -DPICOBIT -DARCH_CPU_RAM_BYTES=0x2000 -DARCH_CPU_ROM_BYTES=0x20000 -mcpu=cortex-m3 -mthumb -g  -Wall -Werror -Os -nostdlib -Larch/arm/cortex-m3/lib -Tstm32f100rb.ld -Wl,-warn-common -fno-hosted -o picobit.elf core/dispatch.c core/heap.c core/gc.c core/primitives-list.c core/primitives-numeric.c core/primitives-util.c core/primitives-vector.c core/primitives-control.c core/bignum_long.c arch/arm/cortex-m3/init.s arch/arm/cortex-m3/board-vldiscovery.c

